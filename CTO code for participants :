/* PARTICIPANTS */
participant Buyer identified by buyerId {
    o String buyerId
    o String firstName 
    o String lastName
    o String permanentAddress
    o DateTime DOB 
    o Integer phone
}
participant Seller identified by sellerId {
    o String sellerId
    o String firstName 
    o String lastName
    o String permanentAddress
    o DateTime DOB 
    o Integer phone
}
participant RegistrationOfficer identified by id {
  o String id
  o String firstName
  o String lastName
  o Integer phone
}
